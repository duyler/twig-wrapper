{
    "name": "duyler/twig-wrapper",
    "type": "library",
    "description": "Duyler twig wrapper",
    "license": "MIT",
    "keywords": [
        "twig"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/docs/twig/"
    },
    "require": {
        "php": "^8.3",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Duyler\\TwigWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\TwigWrapper\\Test\\": ["tests/"]
        }
    }
}
